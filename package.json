{
  "name": "jsconfeu-2019-schedule",
  "version": "1.0.0",
  "description": "JSConf EU 2019 schedule",
  "main": "index.js",
  "private": true,
  "config": {
    "outputFolder": "out",
    "inputUrl": "https://2019.jsconf.eu/schedule/timetable.html",
    "inputUrlBase": "https://2019.jsconf.eu",
    "nowName": "jsconfeu-2019-schedule",
    "nowAlias": "2019-jsconfeu-schedule",
    "nameLong": "JSConf EU 2019 scheduleTODO",
    "nameShort": "jsconfeu19TODO",
    "days": [
      "2019-05-18",
      "2019-06-02"
    ],
    "offlineMode": false
  },
  "scripts": {
    "dev": "node server-dev.js",
    "build": "next build",
    "start": "next start",
    "beautify": "html-beautify $npm_package_config_outputFolder/index.html -r -T style -T script",
    "export": "rm -rf $npm_package_config_outputFolder && NODE_ENV=production next build && next export -o $npm_package_config_outputFolder && rm -rf $npm_package_config_outputFolder/_next && cp src/static/js/sw.js $npm_package_config_outputFolder/ && sed -i -e 's/@VERSION@/'$npm_package_version'/g' $npm_package_config_outputFolder/sw.js && rm -rf $npm_package_config_outputFolder/sw.js-e && npm run beautify",
    "static": "npm run export && node server-static.js",
    "now-deploy": "cd $npm_package_config_outputFolder && now",
    "now-alias": "now alias https://$npm_package_config_nowName.electric-g.now.sh $npm_package_config_nowAlias"
  },
  "author": "Giulia Alfonsi",
  "homepage": "https://github.com/electricg/jsconfeu-2019-scheduleTODO",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.0",
    "js-beautify": "^1.10.0",
    "next": "^8.1.0",
    "raw-loader": "^2.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
